package com.atmecs.chatbox;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.support.ui.Select;

public class HelperClass extends ChatBoxTest {
WebDriver driver;
public HelperClass(WebDriver driver)
{
	this.driver = driver;
}

public void selectElement(String xpath, String value)
{
	Select s=new Select(driver.findElement(By.xpath(prop.getProperty("DropDown"))));
	s.selectByValue("index1_1");
}
}
